<Window x:Class="AlertToCareFrontend.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AlertToCareFrontend"
        xmlns:viewModels="clr-namespace:AlertToCareFrontend.ViewModel"
        mc:Ignorable="d"
        Title="Alert To Care" Height="450" Width="800">
    <Window.Resources>
        <DataTemplate DataType="{x:Type viewModels:HomeViewModel}"></DataTemplate>
        <DataTemplate DataType="{x:Type viewModels:ConfigurationViewModel}"></DataTemplate>
        <DataTemplate DataType="{x:Type viewModels:MonitorIcuViewModel}"></DataTemplate>
    </Window.Resources>
    <DockPanel>
        <ContentControl x:Name="ContentControl" />
    </DockPanel>
</Window>
