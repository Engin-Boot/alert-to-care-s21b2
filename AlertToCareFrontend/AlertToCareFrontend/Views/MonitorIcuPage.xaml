<Page x:Class="AlertToCareFrontend.MonitorIcuPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AlertToCareFrontend"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MonitorIcuPage">

    <DockPanel VerticalAlignment="Center">
        <Button Style="{StaticResource ResourceKey=homeButtonStyle}" Click="BackButton_Click" DockPanel.Dock="Top" HorizontalAlignment="Left">
            <StackPanel Orientation="Horizontal">
                <Image Source="../Resources/icons/home.png" Width="20" Height="20"></Image>
                <TextBlock Text="Back to home" FontSize="12" VerticalAlignment="Center"></TextBlock>
            </StackPanel>
        </Button>
    </DockPanel>
</Page>
